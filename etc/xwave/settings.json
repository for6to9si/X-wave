{
  "client":
  {
    "name": "xray",
    "path_config": "/opt/etc/xray/configs"
  },
  "app":
  {
    "name": "xwave",
    "version": "0.0.3",
    "log": "/opt/var/log/xwave/xwave.log"
  },
  "network":
  {
    "table_id": "224",
    "table_mark_hex": "0x3629",
    "connection_policy": "xwave",
    "chain_name": "xwave",
    "port_redirect": "60018",
    "port_tproxy": "60019",
    "modules":
    [
      "xt_TPROXY.ko",
      "xt_socket.ko",
      "xt_owner.ko"
    ],
    "module_path": "/lib/modules/4.9-ndm-5",
    "port_forwarding_list":
    [
      80,
      443,
      8080
    ],
    "DNS_forwarding": true,
    "IPv6": false,
    "IPv6_exclusions":[
      "2a01:7e00::/29", //IP адреса ваших серверов
      "2001:19f0::/32"
    ],
    "IPv4_exclusions":[
      "104.131.0.0/16", //IP адреса ваших серверов
      "143.42.0.0/16",
      "159.203.0.0/16"
    ]
  }
}
