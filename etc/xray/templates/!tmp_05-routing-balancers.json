                [
                    "redirect",
                    "tproxy",
                    "dns-local",
                    "socks-in"
                ],
                "port": 53,
                "outboundTag": "dns-out"
            },
            {
                "network": "udp",
                "port": "443",
                "outboundTag": "block",
                "type": "field"
            },
            {
                "inboundTag":
                [
                    "redirect",
                    "tproxy",
                    "socks-in"
                ],
                "outboundTag": "block",
                "type": "field",
                "network": "udp",
                "port": "135, 137, 138, 139"
            },
            {
                "inboundTag":
                [
                    "redirect",
                    "tproxy",
                    "socks-in"
                ],
                "xwave-domain": "true",
                "balancerTag": "bestVPN",
                "type": "field",
                "domain":
                [
                    "domain:download-cdn.jetbrains.com",
                    "domain:jetbrains.ai",
                    "domain:plugins.jetbrains.com",
                    "domain:downloads.marketplace.jetbrains.com",
                    "domain:ifconfig.co",
                    "domain:2ip.io",
                    "domain:quic.nginx.org",
                    "domain:vscodium.com"
