            {
                "inboundTag":
                [
                    "redirect",
                    "tproxy",
                    "socks-in"
                ],
                "xwave-ip": "true",
                "balancerTag": "bestVPN",
                "type": "field",
                "ip":
                [
                    "160.79.104.0/21",
                    "178.49.128.0/22"
                ]
            },
            {
                "inboundTag":
                [
                    "redirect",
                    "tproxy",
                    "socks-in"
                ],
                "outboundTag": "direct",
